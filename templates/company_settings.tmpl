    {{ template "scripts.tmpl" . }}

    <script type="text/javascript">
    $('tr#click').click(function() {
        $('#info').removeClass('hide');
        $('#table').addClass('hide');
    });
    $('a#close').click(function() {
        $('#info').addClass('hide');
        $('#table').removeClass('hide');
    });
    </script>

