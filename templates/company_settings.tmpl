                                <ul class="right-sidebar-list ">
                                    <div class="c_content">
                                        <form class="" action="" method="post">
                                            <div class="form-group">
                                                <label for="exampleInput">Name</label>
                                                <input type="text" class="form-control">
                                            </div>
                                            <div class="form-group">
                                                <label for="textarea">Utilization</label>
                                                <textarea class="form-control autosize-animated"></textarea>
                                            </div>
                                            <div class="form-group">
                                                <label for="textarea">Notes</label>
                                                <textarea class="form-control autosize-animated"></textarea>
                                            </div>
                                            <button type="submit" class="btn btn-wet-asphalt" style="bottom:10px; position:fixed;">Submit</button>
                                        </form>
                                    </div>
                                </ul>
                            </div>
                        </div>
                        <div role="tabpanel" class="tab-pane fade in active" id="task">
