							</div>
							<!-- end widget div -->

						</div>

					</article>
					<!-- WIDGET END -->

				</div>

			</div>
			<!-- END MAIN CONTENT -->

		</div>
		<!-- END MAIN PANEL -->

		<div class="modal fade" id="modalRegistrant" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
			<div class="modal-dialog">
				<div class="modal-content">
					<div class="modal-header">
						<button type="button" class="close" data-dismiss="modal" aria-hidden="true">
							&times;
						</button>
						<h4 class="modal-title" id="myModalLabel">Add Registrant</h4>
					</div>
					<div class="modal-body">
						<form class="smart-form">

							<fieldset>
								<section>
									<label class="label">Employee Name:</label>
									<label class="input"><i class="icon-append fa fa-user"></i>
										<input type="text" class="input-sm">
									</label>
								</section>

								<section>
                                    <div class="form-group">
                                        <label>Phone</label>
                                        <div class="input-group">
                                            <input type="text" class="form-control" data-mask="(999) 999-9999" data-mask-placeholder= "">
                                            <span class="input-group-addon"><i class="fa fa-phone"></i></span>
                                        </div>
                                    </div>
								</section>

                                <section>
                                    <label class="label">Branch</label>
                                    <label class="select">
                                        <select>
                                            <option value="">Baltimore</option>
                                        </select> <i></i>
                                    </label>
                                </section>

                                <section>
                                    <label class="label">Status</label>
                                    <div class="row">
                                        <div class="col col-4">
                                            <label class="checkbox">
                                                <input type="checkbox" name="checkbox" checked="checked">
                                                <i></i>Registered</label>
                                            <label class="checkbox">
                                                <input type="checkbox" name="checkbox">
                                                <i></i>Not Registered</label>
                                            <label class="checkbox">
                                                <input type="checkbox" name="checkbox">
                                                <i></i>Completed</label>
                                        </div>
                                    </div>
                                </section>

                                <div class="form-group">
                                    <label>Registrant Number</label>
                                    <div class="input-group">
                                        <input type="text" class="form-control" data-mask="9999-9999" data-mask-placeholder= "">
                                        <span class="input-group-addon"><i class="fa fa-briefcase"></i></span>
                                    </div>
                                </div>

								<section>
									<label class="label">Notes:</label>
									<label class="textarea textarea-resizable">
										<textarea rows="3" class="custom-scroll"></textarea>
									</label>
								</section>

							</fieldset>

						</form>
					</div>
					<div class="modal-footer">
						<button type="button" class="btn btn-default" data-dismiss="modal">
							Cancel
						</button>
						<button type="button" class="btn btn-primary">
							Submit
						</button>
					</div>
				</div>
			</div>
		</div>

		<div class="modal fade" id="modalTask" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
			<div class="modal-dialog">
				<div class="modal-content">
					<div class="modal-header">
						<button type="button" class="close" data-dismiss="modal" aria-hidden="true">
							&times;
						</button>
						<h4 class="modal-title" id="myModalLabel">Add Job Task</h4>
					</div>
					<div class="modal-body">
						<form class="smart-form">

							<fieldset>
								<section>
									<label class="label">Task:</label>
									<label class="input">
										<input type="text" class="input-sm">
									</label>
								</section>

								<section>
									<label class="label">Information Name:</label>
									<label class="input">
										<input type="text" class="input-sm">
									</label>
								</section>

							</fieldset>

						</form>
					</div>
					<div class="modal-footer">
						<button type="button" class="btn btn-default" data-dismiss="modal">
							Cancel
						</button>
						<button type="button" class="btn btn-primary">
							Submit
						</button>
					</div>
				</div>
			</div>
		</div>

		{{ template "footer.tmpl" . }}

		{{ template "scripts.tmpl" . }}

		<script type="text/javascript">
	    $('tr#click').click(function() {
	        $('#info').removeClass('hide');
	        $('#table').addClass('hide');
	    });
	    $('button#close').click(function() {
	        $('#info').addClass('hide');
	        $('#table').removeClass('hide');
	    });
	    </script>


	</body>

</html>
